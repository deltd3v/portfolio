{
	"name": "new",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"start": "npm run format && npm run k && npm run clean && react-scripts start",
		"build": "react-scripts build",
		"serve": "npm run k && serve -s build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"clean": "rimraf -f build",
		"update": "ncu -u && npm install",
		"k": "kill-port --port 3000,5000--method tcp,http,https",
		"format": "prettier --write '**/**/**/*.{ts,tsx,json,css,scss}'"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/react": "^17.0.30",
		"@types/react-dom": "^17.0.9",
		"kill-port": "^1.6.1",
		"node-sass": "^6.0.1",
		"npm-check-updates": "^11.8.5",
		"prettier": "^2.4.1",
		"rimraf": "^3.0.2",
		"serve": "^12.0.1",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.1.2",
		"@testing-library/user-event": "^13.5.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "4.0.3",
		"web-vitals": "^2.1.2"
	}
}
